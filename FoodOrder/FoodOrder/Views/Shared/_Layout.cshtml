<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
  
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/Content/custom/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/custom/vendors/bootstrap-selector/css/bootstrap-select.min.css">
    <!--icon font css-->
    <link rel="stylesheet" href="~/Content/custom/vendors/themify-icon/themify-icons.css">
    <link rel="stylesheet" href="~/Content/custom/vendors/flaticon/flaticon.css">
    <link rel="stylesheet" href="~/Content/custom/vendors/animation/animate.css">
    <link rel="stylesheet" href="~/Content/custom/vendors/owl-carousel/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/custom/vendors/magnify-pop/magnific-popup.css">
    <link rel="stylesheet" href="~/Content/custom/vendors/nice-select/nice-select.css">
    <link rel="stylesheet" href="~/Content/custom/vendors/elagent/style.css">
    <link rel="stylesheet" href="~/Content/custom/vendors/scroll/jquery.mCustomScrollbar.min.css">
    <link rel="stylesheet" href="~/Content/custom/style.css">
    <link rel="stylesheet" href="~/Content/custom/responsive.css">

</head>
<body>

    <div id="preloader">
        <div id="ctn-preloader" class="ctn-preloader">
            <div class="animation-preloader">

                <div class="txt-loading">
                    <img src="~/Content/img/load.gif" alt="">

                </div>

            </div>
            <div class="loader">
                <div class="row">
                    <div class="col-3 loader-section section-left">
                        <div class="bg"></div>
                    </div>
                    <div class="col-3 loader-section section-left">
                        <div class="bg"></div>
                    </div>
                    <div class="col-3 loader-section section-right">
                        <div class="bg"></div>
                    </div>
                    <div class="col-3 loader-section section-right">
                        <div class="bg"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="body_wrapper">
        <header class="header_area">

            <nav class="navbar navbar-expand-lg menu_two">
                <div class="container">
                    <img src="~/Content/img/logo.png" srcset="img/logo2x.png 2x" alt="logo">
                    <button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="menu_toggle">
                            <span class="hamburger">
                                <span></span>
                                <span></span>
                                <span></span>
                            </span>
                            <span class="hamburger-cross">
                                <span></span>
                                <span></span>
                            </span>
                        </span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-center" id="navbarSupportedContent">
                        <ul class="navbar-nav menu w_menu">
                            <li class="nav-item dropdown submenu mega_menu mega_menu_two active">
                                <a class="nav-link dropdown-toggle" href="serv" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    О сервисе
                                </a>
                            </li>
                            <li class="dropdown submenu nav-item">
                                <a title="Pages" class="dropdown-toggle nav-link" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false" href="screen">Скриншоты</a>
                            </li>
                            <li class="nav-item dropdown submenu mega_menu">
                                <a class="nav-link dropdown-toggle" href="menu" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    Меню
                                </a>
                            </li>
                            <li class="nav-item dropdown submenu mega_menu">
                                <a class="nav-link dropdown-toggle" href="preim" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    Преимущества
                                </a>
                            </li>
                        </ul>
                    </div>

                    <div class="nav_right_btn">
                        <a id="orCl" style="margin-right:5px" href="ord" class="login_btn active menu_cus"><i style="margin-right:4px" class="ti-plus"></i>Подключить сервис</a>
                        <a href="/Account/Login" class="login_btn active">Войти</a>
                    </div>
                </div>
            </nav>
        </header>
        @RenderBody()
        <footer class="footer_area_two">

            <div class="footer_bottom">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-6">
                            <p class="mb-0 f_400">Copyright © 2019 Сделано в <a href="#">Студия №666</a></p>
                        </div>
                        <div class="col-sm-6">
                            <ul class="list-unstyled f_menu text-right">
                                <li><a href="#">Правило использования</a></li>
                                <li><a href="#">Соглашение</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    @RenderSection("scripts", required: false)

    <script src="~/Scripts/js/jquery-3.2.1.min.js"></script>
    <script src="~/Scripts/js/propper.js"></script>
    <script src="~/Scripts/js/bootstrap.min.js"></script>
    <script src="~/Content/custom/vendors/bootstrap-selector/js/bootstrap-select.min.js"></script>
    <script src="~/Content/custom/vendors/wow/wow.min.js"></script>
    <script src="~/Content/custom/vendors/sckroller/jquery.parallax-scroll.js"></script>
    <script src="~/Content/custom/vendors/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/Content/custom/vendors/nice-select/jquery.nice-select.min.js"></script>
    <script src="~/Content/custom/vendors/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="~/Content/custom/vendors/isotope/isotope-min.js"></script>
    <script src="~/Content/custom/vendors/counterup/jquery.counterup.min.js"></script>
    <script src="~/Content/custom/vendors/counterup/jquery.waypoints.min.js"></script>
    <script src="~/Content/custom/vendors/magnify-pop/jquery.magnific-popup.min.js"></script>
    <script src="~/Content/custom/vendors/scroll/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Scripts/js/main.js"></script>

    <script>
        function goToByScroll(id) {
            $('html,body').animate({
                scrollTop: $("#" + id).offset().top
            },
                'slow');
        }

        $("#orCl").click(function (e) {
            e.preventDefault();
            goToByScroll($(this).attr("href"));
        });

        $("#navbarSupportedContent > ul > li > a").click(function (e) {
            // Prevent a page reload when a link is pressed
            e.preventDefault();
            // Call the scroll function
            goToByScroll($(this).attr("href"));
        });

    </script>


</body>
</html>
