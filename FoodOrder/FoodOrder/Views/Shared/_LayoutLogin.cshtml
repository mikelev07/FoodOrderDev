    <script src="~/Content/custom/vendors/isotope/isotope-min.js"></script>
    <script src="~/Content/custom/vendors/magnify-pop/jquery.magnific-popup.min.js"></script>
    <script src="~/Content/custom/vendors/scroll/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Scripts/js/main.js"></script>
</body>

</html>
