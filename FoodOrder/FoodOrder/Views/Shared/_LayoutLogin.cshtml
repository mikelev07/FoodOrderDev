<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    <title></title>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/Content/custom/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/custom/vendors/bootstrap-selector/css/bootstrap-select.min.css">
    <!--icon font css-->
    <link rel="stylesheet" href="~/Content/custom/vendors/themify-icon/themify-icons.css">
    <link rel="stylesheet" href="~/Content/custom/vendors/flaticon/flaticon.css">
